  "engines": {
    "node": ">=12.0.0"
  },
  "author": "NeoYung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YunNeo/vite-plugin-react-imp/issues"
  },
  "homepage": "https://github.com/YunNeo/vite-plugin-react-imp#readme",
  "peerDependencies": {
    "vite": ">= 2.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.14.1",
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "vite": "^2.2.4"
  },
  "dependencies": {
    "@babel/parser": "^7.14.1",
